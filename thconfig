encoding  utf-8
language ru

source sliz.th

layout normal
     symbol-set BCRA
     #scale 1 200
     doc-author "Paweł Krawczyk"
     doc-title "Слизняки"
     statistics explo all  
     statistics topo all
     statistics carto all
     statistics copyright all
     legend on
     map-comment "Сары Тала"
     colour map-fg altitude
     symbol-hide group cave-centreline
endlayout

select m1s@sliz
select m1p@sliz


export map -format pdf -projection extended -layout normal -o sliznyak-sect.pdf 
export map -format svg -projection extended -layout normal -o sliznyak-sect.svg


export map -format pdf -layout normal -o sliznyak-plan.pdf 
export map -format svg -layout normal -o sliznyak-plan.svg
